{
    "name": "craftsnippets/table-of-contents",
    "description": "This plugin generates a table of contents from HTML headers in text.",
    "type": "craft-plugin",
    "version": "2.0.0",
    "keywords": [
        "craft cms",
        "craftcms",
        "table of contents"
    ],
    "support": {
        "docs": "https://github.com/craft-snippets/craft-table-of-contents/blob/master/README.md",
        "issues": "https://github.com/craft-snippets/craft-table-of-contents/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Piotr Pogorzelski",
            "homepage": "http://craftsnippets.com",
            "email": "piotrpog@protonmail.com"
        }
    ],
    "require": {
        "craftcms/cms": "^4.0.0"
    },
    "autoload": {
        "psr-4": {
          "craftsnippets\\tableofcontents\\": "src/"
        }
    },
    "extra": {
        "name": "Table of contents",
        "handle": "table-of-contents",
        "hasCpSettings": false,
        "hasCpSection": false,
        "changelogUrl": "https://github.com/craft-snippets/craft-table-of-contents/blob/master/CHANGELOG.md",
        "class": "craftsnippets\\tableofcontents\\TableOfContents"
    }
}
